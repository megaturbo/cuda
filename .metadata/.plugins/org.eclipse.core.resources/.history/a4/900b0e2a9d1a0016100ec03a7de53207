#include <iostream>

using std::cout;
using std::endl;

#include "Histogram.h"

bool useHistogram(void);

bool useHistogram()
{
	int tabSize = 2487;
	float result;

	Histogram histogram(tabSize);
	result = histogram.run();

	cout << "Result =  " << result << endl;

	return true;	// todo beterrerer
}
