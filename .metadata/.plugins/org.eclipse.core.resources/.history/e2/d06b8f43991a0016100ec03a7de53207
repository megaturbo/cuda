#include <iostream>

using std::cout;
using std::endl;

/* ========== IMPORTED ========== */

#include "histogram.h"

/* ========== DECLARATION ========== */
/* ========== PUBLIC ========== */

/* ========== IMPLEMENTATION ========== */
/* ========== PUBLIC ========== */
bool useHistogram(void);

bool useHistogram()
{
	int tabSize = 256;
	float result;

	Histogram histogram(tabSize);
	result = histogram.run();

	cout << "Frequence moyenne " << result << endl;

	return true;
}
